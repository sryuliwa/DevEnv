        { "command": "copy", "keys": ["ctrl+shift+c"] },
        { "command": "newTab", "keys": ["ctrl+shift+t"] },
        { "command": "nextTab", "keys": ["alt+right"] },
        { "command": "prevTab", "keys": ["alt+left"] },
        { "command": "splitHorizontal", "keys": ["ctrl+shift+-"] },
        { "command": "splitVertical", "keys": ["ctrl+shift+E"] },
        { "command": {"action":"moveFocus","direction":"left"}, "keys": ["shift+left"] },
        { "command": {"action":"moveFocus","direction":"right"}, "keys": ["shift+right"] },
